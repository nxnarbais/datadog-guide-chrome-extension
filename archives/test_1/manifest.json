{
  "manifest_version": 2,
  "name": "DDog Guide",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },  
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
